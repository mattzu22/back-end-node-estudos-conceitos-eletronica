generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Part {
  id        String   @id @default(uuid())
  model     String   
  brand     String
  type      String
  state     Boolean?
  qauntity    Int?
  createdAt DateTime @default(now())

  @@map("parts")
}
